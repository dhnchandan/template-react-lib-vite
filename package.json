{
  "name": "template-react-lib-vite",
  "private": true,
  "version": "0.0.0",
  "description": "Starting reactjs library template with vite.",
  "author": "DHN Chandan",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-lib.umd.cjs",
  "module": "./dist/react-lib.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-lib.js",
      "require": "./dist/react-lib.umd.cjs"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhnchandan/template-react-lib-vite.git"
  },
  "bugs": {
    "email": "fly.dev.org@gmail.com",
    "url": "https://github.com/dhnchandan/template-react-lib-vite/issues"
  },
  "homepage": "https://github.com/dhnchandan/template-react-lib-vite.git",
  "keywords": [
    "typescript",
    "javascript",
    "react",
    "library",
    "npm",
    "vite"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "pathe": "^1.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3"
  }
}